{
  "name": "darwin",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/TheRigelCms/Darwin.git",
  "author": "phil <philippe.tedajo@gmail.com>, ltphen<tatkeulionel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "db:seed": "prisma db seed --preview-feature",
    "db:migrate": "prisma migrate dev --name init",
    "db:studio": "npx prisma studio",
    "start": "concurrently \"yarn run tsoa:watch\" \"yarn run bundle:watch\" \"yarn run run:watch\"",
    "dep:check": "node bin/checkDep.js",
    "run": "node build/index.js",
    "run:watch": "nodemon --watch build --delay 2000ms build/index.js",
    "tsoa": "tsoa spec-and-routes -c ./tsoa.json",
    "tsoa:watch": "nodemon --watch ./src/**/*.controller.ts --watch ./tsoa.json -e ts,json --exec yarn run tsoa",
    "bundle": "webpack --config webpack.config.js",
    "bundle:watch": "yarn run bundle --watch",
    "build": "yarn run tsoa && yarn run bundle --progress",
    "build:release": "yarn run tsoa && webpack --config webpack.config.js --progress --colors --env.release",
    "jest": "jest",
    "tsc": "tsc && rm -rf build",
    "test": ""
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "nodemon": "^2.0.12",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@types/readline-sync": "^1.4.4",
    "@types/swagger-ui-express": "^4.1.3",
    "depcheck": "^1.4.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "readline-sync": "^1.4.10",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2"
  }
}
